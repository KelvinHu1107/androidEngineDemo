    <dimen name="compass_size">70dp</dimen>
    <dimen name="entry_info_size">200dp</dimen>
    <dimen name="map_move_icon_size">50dp</dimen>
    <dimen name="remain_dist_group_height">50dp</dimen>
    <dimen name="remain_dist_group_width">100dp</dimen>
    <dimen name="speed_digit_height">35dp</dimen>
    <dimen name="speed_digit_width">22dp</dimen>
    <dimen name="speed_panel_height">80dp</dimen>
    <dimen name="speed_panel_width">150dp</dimen>
    <dimen name="speed_unit_height">15dp</dimen>
    <dimen name="speed_unit_text_height">11dp</dimen>
    <string name="action_settings">Settings</string>
    <string name="app_name">RousenR13</string>
    <string name="hello_world">Hello world!</string>
    <string name="menu_settings">Settings</string>
    <string name="title_activity_car_test">CarTestActivity</string>
    <string name="title_activity_ex_setting">ExSettingActivity</string>
    <string name="title_activity_poi_icon_test">PoiIconTest</string>
    <string name="title_activity_route">RouteActivity</string>
    <string name="title_activity_route_config">RouteConfigActivity</string>
    <string name="title_activity_route_detail">RouteDetailActivity</string>
    <string name="title_activity_route_list">RouteListActivity</string>
    <string name="title_activity_start">Start</string>
    <style name="AppTheme" parent="android:Theme.Light"/>
    <style name="btn_back">
	 	<item name="android:id">@+id/btnBack</item>
	 	<item name="android:layout_width">85px</item>
	 	<item name="android:layout_height">60px</item>
	 	<item name="android:layout_centerVertical">true</item>
	 	<item name="android:layout_marginLeft">5dp</item>
	 	<item name="android:layout_marginRight">5dp</item>
	 	<item name="android:background">@drawable/xml_btn_topback</item>
	</style>
    <style name="btn_home">
	 	<item name="android:id">@+id/btnHome</item>
	 	<item name="android:layout_width">78px</item>
	 	<item name="android:layout_height">60px</item>
	 	<item name="android:layout_alignParentRight">true</item>
	 	<item name="android:layout_centerVertical">true</item>
	 	<item name="android:background">@drawable/xml_btn_gomap</item>
	 	<item name="android:padding">0dp</item>
	 	<item name="android:text">Map</item>
	 	<item name="android:textColor">@color/textcolor</item>
	 	<item name="android:textSize">12sp</item>
	</style>
    <style name="btn_icon">
	 	<item name="android:layout_width">wrap_content</item>
	 	<item name="android:layout_height">wrap_content</item>
	 	<item name="android:textColor">@color/textcolor</item>
	 	<item name="android:gravity">right|center_vertical</item>
	 	<item name="android:paddingBottom">0dp</item>
	 	<item name="android:paddingLeft">0dp</item>
	 	<item name="android:paddingRight">5dp</item>
	 	<item name="android:paddingTop">0dp</item>
	 	<item name="android:textScaleX">0.9</item>
	</style>
    <style name="btn_normal">
	 	<item name="android:layout_width">wrap_content</item>
	 	<item name="android:layout_height">wrap_content</item>
	 	<item name="android:padding">0dp</item>
	 	<item name="android:background">@drawable/xml_btn_blue</item>
	 	<item name="android:textColor">@color/textcolor</item>
	</style>
    <style name="layout_title">
	 	<item name="android:id">@+id/titleLayout</item>
	 	<item name="android:layout_width">fill_parent</item>
	 	<item name="android:layout_height">74px</item>
	 	<item name="android:layout_alignParentLeft">true</item>
	 	<item name="android:background">@drawable/top_back</item>
	</style>
    <style name="txt_title">
	 	<item name="android:id">@+id/txtPNSTitle</item>
	 	<item name="android:layout_width">wrap_content</item>
	 	<item name="android:layout_height">wrap_content</item>
	 	<item name="android:layout_centerInParent">true</item>
	 	<item name="android:textColor">#FFFFFFFF</item>
	 	<item name="android:textSize">20sp</item>
	</style>
</resources>